using System;

namespace char_demo
{
    class Program
    {
        static void Main(string[] args)
        {
            //Console.WriteLine("Hello World!");
            char myFirstInitial = 'A';
            char myLastInitial = 'K';

            char tab = '\t';
            char newLine = '\n';

            char hexLetter = '\u004A';

            Console.WriteLine("My First Initial:" + tab + myFirstInitial + newLine + "My Last Initial:" + tab + myLastInitial);
            Console.WriteLine("u004a hex value gives: " + hexLetter);

        }

    }
}
